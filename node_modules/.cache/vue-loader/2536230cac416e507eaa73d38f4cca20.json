84\",\r\n      salt: 2,\r\n      secret_key: \"VQOOgSdrRIP4ZckPb0lT\",\r\n    };\r\n  },\r\n  methods: {\r\n    changeSelectLeft() {\r\n      if (this.selectValue1 == \"zh\") {\r\n        this.selectValue1 = \"zh\";\r\n        this.selectValue2 = \"en\";\r\n        this.textLeft = \"请输入中文\";\r\n        this.textRight = \"Please input English\";\r\n      } else {\r\n        this.selectValue1 = \"en\";\r\n        this.selectValue2 = \"zh\";\r\n        this.textLeft = \"Please input English\";\r\n        this.textRight = \"请输入中文\";\r\n      }\r\n    },\r\n    changeSelectRight() {\r\n      if (this.selectValue2 == \"en\") {\r\n        this.selectValue1 = \"zh\";\r\n        this.selectValue2 = \"en\";\r\n        this.textLeft = \"请输入中文\";\r\n        this.textRight = \"Please input English\";\r\n      } else {\r\n        this.selectValue1 = \"en\";\r\n        this.selectValue2 = \"zh\";\r\n        this.textLeft = \"Please input English\";\r\n        this.textRight = \"请输入中文\";\r\n      }\r\n    },\r\n    translate() {\r\n      this.$http\r\n        .get(\r\n          \"https://api.fanyi.baidu.com/api/trans/vip/translate\" +\r\n            \"?q=\" +\r\n            this.textareaLeft +\r\n            \"&from=\" +\r\n            this.selectValue1 +\r\n            \"&to=\" +\r\n            this.selectValue2 +\r\n            \"&appid=\" +\r\n            this.appKey +\r\n            \"&salt=\" +\r\n            this.salt +\r\n            \"&sign=\" +\r\n            this.$md5(\r\n              this.appKey + this.textareaLeft + this.salt + this.secret_key\r\n            )\r\n        )\r\n        .then((res) => {\r\n          debugger;\r\n          this.textareaRight = res.data.trans_result[0].dst;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.line {\r\n  text-align: center;\r\n}\r\n.el-row {\r\n  margin-bottom: 20px;\r\n}\r\n.el-col {\r\n  border-radius: 4px;\r\n}\r\n.grid-content {\r\n  border-radius: 4px;\r\n  min-height: 336px;\r\n}\r\n.row-bg {\